title: 'Sessions'
methods:
 -
   title: 'Login'
   path: '/sessions'
   method: 'post'
   parameters:
     -
       name: 'email'
       type: 'string'
       required: true
     -
       name: 'password'
       type: 'string'
       required: true
   example_responses:
     -
       code: 200
       response: var:session_and_user
       type: 'json'
     -
       code: 401
       response: >
         {
           "errors": [
             "Invalid Email or Password. Please check the entered data."
           ]
         }
       type: 'json'
 -
   title: 'Logout'
   path: '/sessions/logout'
   method: 'delete'
   headers:
     -
       name: 'Session-Token'
   example_responses:
     -
       code: 200
       response: var:ok
       type: 'json'
     -
       code: 401
       response: var:access_denied_error
       type: 'json'
