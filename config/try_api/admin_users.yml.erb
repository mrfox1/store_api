title: 'Users Admin API'
methods:
  -
    title: "Users list"
    path: "/admin/admin_users"
    method: "GET"
    headers:
      -
        name: 'Session-Token'
    example_responses:
      -
        code: 200
        response: var:users_list
        type: 'json'
